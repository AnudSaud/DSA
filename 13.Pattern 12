#include <iostream>
using namespace std;

int main() {
    int i,j,n;
    
    
    cout<<"Enter Rows"<<endl;
    cin>>n;
    
  for(i=1; i<=n; i++)
    {   
        for(j=i; j>0; j--)
         {    
             char ch='A'+i-1;
             cout<<ch<<" ";
            
         }
          cout<<endl;
    }
    return 0;
}
