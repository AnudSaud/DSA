#include <iostream>
using namespace std;

int main() {
    int i,j,n,m,count=1;
    cout<<"Enter Rows"<<endl;
    cin>>n;
    
   // cout<<"Enter Columns"<<endl;
   // cin>>m;
    
     
   for(i=1; i<=n; i++)
    {   
        for(j=i; j>0; j--)
         {
             cout<<j;
             count++;
         }
          cout<<endl;
         
    }
    return 0;
}
