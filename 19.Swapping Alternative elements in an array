#include <iostream>

using namespace std;

int main()
{
    int a[10]={1,2,3,4,5,6,7,8,9,1};
    
    
    cout<<"After swapping alternative elements"<<endl;
    
    for(int i=0; i<10; i++)
     {
        int temp=a[i];
            a[i]=a[i+1];
            a[i+1]=temp;
            i++;
           
     }
     
     for(int i=0; i<10; i++)
       {
           cout<<a[i];
       }
   
         
     
    
     
}
